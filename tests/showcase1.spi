open corebase
open corecuda
open coreext

open tensorm

inl main() =
    inl t : tensor (int * int) float = cupy.random_normal {mean=0; std=1} (5,5)
    console.write_ln "Printing this on host."
    console.write_ln t
    run fun () =>
        if rangem.threads_in_grid().from = 0 then
            console.write_ln "Printing this on the device."
            console.write_ln t