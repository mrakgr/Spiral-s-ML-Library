open real_core

nominal add a b = `(lit_to_type_lit (type_lit_to_lit `a + type_lit_to_lit `b))
nominal sub a b = `(lit_to_type_lit (type_lit_to_lit `a - type_lit_to_lit `b))
nominal mult a b = `(lit_to_type_lit (type_lit_to_lit `a * type_lit_to_lit `b))
nominal div a b = `(lit_to_type_lit (type_lit_to_lit `a / type_lit_to_lit `b))
nominal divup a b = `(lit_to_type_lit (divup (type_lit_to_lit `a) (type_lit_to_lit `b)))